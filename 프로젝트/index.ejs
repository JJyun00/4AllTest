<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <title>User list</title>
</head>
<body>
    <table>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Tel.</th>
            <!-- <th>수정</th>
            <th>삭제</th> -->
        </tr>
        <% info.forEach(function (value) { %>
        <tr>
            <td><%= value.name %></td>
            <td><%= value.email %></td>
            <td><%= value.tel %></td>
            <!-- <td><a href="/edit/<%= value.id %>">수정</a></td>
            <td><a href="/delete/<%= value.id %>">삭제</a></td> -->
        </tr>
        <% }); %>
    </table>
</body>
</html>